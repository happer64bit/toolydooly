{
  "name": "email-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "node:dev": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "dev": "tsx --watch ./src/index.ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\" --max-warnings 0",
    "start": "node -r esbuild-register ./src/index.ts"
  },
  "dependencies": {
    "@types/express-http-proxy": "^1.6.7",
    "amqplib": "^0.10.9",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "http-proxy-middleware": "^3.0.5",
    "morgan": "^1.10.0",
    "@toolydooly/typescript-config": "*",
    "nodemailer": "^7.0.6"
  },
  "devDependencies": {
    "@toolydooly/eslint-config": "*",
    "@toolydooly/typescript-config": "*",
    "@types/amqplib": "^0.10.7",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/nodemailer": "^7.0.1",
    "esbuild": "^0.20.1",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "typescript": "5.5.4"
  }
}
