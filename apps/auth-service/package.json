{
  "name": "auth-service",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "node:dev": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "dev": "tsx --watch ./src/index.ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\" --max-warnings 0",
    "start": "node -r esbuild-register ./src/index.ts",
    "test": "jest --detectOpenHandles",
    "migrate": "knex --knexfile src/knexfile.ts migrate:latest",
    "rollback": "knex --knexfile src/knexfile.ts migrate:rollback"
  },
  "jest": {
    "preset": "@toolydooly/jest-presets/node"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "redis": "^5.8.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@toolydooly/eslint-config": "*",
    "@toolydooly/jest-presets": "*",
    "@toolydooly/typescript-config": "*",
    "@toolydooly/validation-schemas": "*",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.2",
    "esbuild": "^0.20.1",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.3",
    "tsx": "^4.20.5",
    "typescript": "5.5.4"
  },
  "packageManager": "bun@1.2.21"
}
